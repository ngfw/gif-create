{
    "name": "pomirleanu/gif-create",
    "type": "library",
    "description": "Easy way to create gif animations from png images.",
    "keywords": [
        "pomirleanu",
        "GifCreate"
    ],
    "homepage": "https://github.com/pomirleanu/gif-create",
    "license": "MIT",
    "authors": [
        {
            "name": "Pomirleanu Florentin Cristinel",
            "email": "pomirleanu.florentin@gmail.com",
            "homepage": "http://printreblocuri.ro",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1||~6.0",
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Pomirleanu\\GifCreate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pomirleanu\\GifCreate\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
